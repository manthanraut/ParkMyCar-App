<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkmycar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.parkmycar.about"/>
        <activity android:name="com.example.parkmycar.guide"/>
        <activity android:name="com.example.parkmycar.myhistory"/>
        <activity android:name="com.example.parkmycar.feedbackform"/>
        <activity android:name="com.example.parkmycar.profilepage"/>
        <activity android:theme="@style/AppTheme_NoActionBar" android:name="com.example.parkmycar.HomeScreen"/>
        <activity android:name="com.example.parkmycar.loginDriver"/>
        <activity android:name="com.example.parkmycar.registrationDriver"/>
    </application>

</manifest>